<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".NewActivity">

    <androidx.constraintlayout.utils.widget.ImageFilterButton
        android:id="@+id/imageFilterButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/air1" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="match_parent"
        android:layout_height="84dp"
        android:layout_above="@+id/button1"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="54dp"
        android:text="JET TECH"
        android:textAlignment="center"
        android:textSize="45sp"
        android:textStyle="bold|italic" />

    <Button
        android:id="@+id/button1"
        android:layout_width="230dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button2"
        android:layout_marginEnd="2dp"
        android:layout_marginRight="2dp"
        android:layout_marginBottom="102dp"
        android:layout_toStartOf="@+id/imageButton"
        android:layout_toLeftOf="@+id/imageButton"
        android:background="#636363"
        android:onClick="books"
        android:text="Book Flight"
        android:textColor="@color/black"
        android:textSize="25sp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="248dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button3"
        android:layout_marginEnd="-8dp"
        android:layout_marginRight="-8dp"
        android:layout_marginBottom="38dp"
        android:layout_toStartOf="@+id/imageButton2"
        android:layout_toLeftOf="@+id/imageButton2"
        android:background="#636363"
        android:text="View Booking"
        android:textColor="@color/black"
        android:textSize="25sp"
        android:onClick="booms"/>

    <Button
        android:id="@+id/button3"
        android:layout_width="239dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button8"
        android:layout_marginEnd="-2dp"
        android:layout_marginRight="-2dp"
        android:layout_marginBottom="45dp"
        android:layout_toStartOf="@+id/imageButton3"
        android:layout_toLeftOf="@+id/imageButton3"
        android:background="#636363"
        android:text="View Details"
        android:textColor="@color/black"
        android:textSize="25sp"
        android:onClick="detailing"/>

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageButton2"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="127dp"
        android:layout_marginRight="127dp"
        android:layout_marginBottom="102dp"
        app:srcCompat="@drawable/ic_bookflight"
        tools:ignore="VectorDrawableCompat" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageButton3"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="118dp"
        android:layout_marginRight="118dp"
        android:layout_marginBottom="41dp"
        app:srcCompat="@drawable/ic_viewbooking"
        tools:ignore="VectorDrawableCompat" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageButton4"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="119dp"
        android:layout_marginRight="119dp"
        android:layout_marginBottom="48dp"
        app:srcCompat="@drawable/ic_viewdetails"
        tools:ignore="VectorDrawableCompat" />

    <Button
        android:id="@+id/button8"
        android:layout_width="229dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="9dp"
        android:layout_marginRight="9dp"
        android:layout_marginBottom="91dp"
        android:layout_toStartOf="@+id/imageButton4"
        android:layout_toLeftOf="@+id/imageButton4"
        android:background="#636363"
        android:text="Logout"
        android:textColor="@color/black"
        android:textSize="25sp"
        android:onClick="ClickLogout"/>

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="118dp"
        android:layout_marginRight="118dp"
        android:layout_marginBottom="93dp"
        app:srcCompat="@drawable/ic_logout"
        tools:ignore="VectorDrawableCompat" />
</RelativeLayout>